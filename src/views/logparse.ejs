<!DOCTYPE html>
<html lang="en">

<head>

  <title>Log Parser</title>

</head>

<body id="page-top">

  <!-- Events -->
  <section class="page-section">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <select class="browser-default custom-select" id="logType" onchange="checkmode()">
              <option selected value='Joomla'>Joomla (default)</option>
              <option value='IIS'>IIS</option>
          </select>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <select class="browser-default custom-select" id="modeType" disabled="true">
              <option selected value='detail'>detailed</option>
              <option value='summurl'>summary (IP & URL)</option>
              <option value='summstat'>summary (IP & status)</option>
              <option value='summip'>summary (IP only)</option>
          </select>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <select class="browser-default custom-select" id="botType" disabled="true">
              <option selected value='default'>IP & agent</option>
              <option value='agent'>agent only</option>
              <option value='ip'>IP only</option>
          </select>
          <button id="submitbutton" onclick="logparse()">submit</button>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <label for="email">Email Output?</label>
          <input type="checkbox" id="email" onclick="checkmail()">
          <input type="text" id="emailaddress" disabled="true">
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
        <textarea rows="3" id="results" readonly style="border: none"></textarea>
        </div>
      </div>
    </div>
  </section> 

  <script src="logparse-process.js"></script>
  <script src="/socket.io/socket.io.js"></script>
</body>

</html>
